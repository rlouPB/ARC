<aura:component implements="lightning:actionOverride">
    <aura:attribute name="message" type="String" description="Message to display"  access="public" 
                    default="To create a new Request Item, please access the Patient file and create the Request item from within Pre-Admission Requirements."/>
<div class="slds-page-header " >
    <div class="slds-media slds-media_center" style="min-height: 150px;">
        <div class="slds-media__figure">
            <lightning:icon iconName="utility:warning" alternativeText="Warning!" variant="warning" />
        </div>
        <div class="slds-media__body">
            <p class="slds-text-heading_medium "><strong>{!v.message}</strong></p>
        </div>
	    
        
    </div>
</div>	
</aura:component>